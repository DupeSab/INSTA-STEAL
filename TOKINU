-- ==== FAKE TOGGLE =====================================================
local enabled = false
local function UpdateButton()
    enabled = not enabled
    Button.Text = enabled and "Instant Steal\nACTIVE" or "Instant Steal\nR To Activate"
    Grad.Color = enabled and ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.fromRGB(0,80,0)), ColorSequenceKeypoint.new(1,Color3.fromRGB(0,120,20))} or ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.fromRGB(40,40,40)), ColorSequenceKeypoint.new(1,Color3.fromRGB(60,60,60))}
end
Button.MouseButton1Click:Connect(UpdateButton)

-- ==== SAVE CLAIM POSITION ==============================================
local function saveClaimZone()
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        MY_CLAIM_POS = LocalPlayer.Character.HumanoidRootPart.Position
        Info.Text = "Claim saved!"
        task.wait(1)
        Info.Text = "Press R to teleport + steal"
    else
        Info.Text = "No character!"
    end
end
SetLocBtn.MouseButton1Click:Connect(saveClaimZone)

-- ==== MAIN R FUNCTION (TELEPORT + SHOOT + STEAL + LOCK) ===============
local function claimStealAndLock()
    if not MY_CLAIM_POS or active then return end
    active = true
    
    task.spawn(function()
        Info.Text = "Teleporting..."
        
        local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart", 5)
        if not hrp then Info.Text = "No HRP!"; active = false; return end
        
        hrp.CFrame = CFrame.new(MY_CLAIM_POS)
        Info.Text = "Teleported!"
        task.wait(0.5)
        
        -- SHOOT
        Info.Text = "Shooting..."
        local target, minDist = nil, math.huge
        for _, p in Players:GetPlayers() do
            if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                local d = (p.Character.HumanoidRootPart.Position - hrp.Position).Magnitude
                if d < minDist then minDist = d; target = p.Character.HumanoidRootPart end
            end
        end
        if target then
            local gun = char:FindFirstChild("Laser Gun") or LocalPlayer.Backpack:FindFirstChild("Laser Gun")
            if gun then
                gun.Parent = char
                task.wait(0.2)
                local remote = gun:FindFirstChild("Remote") or gun:FindFirstChild("Fire") or gun:FindFirstChild("Event")
                if remote and remote:IsA("RemoteEvent") then
                    for i = 1, SHOTS do
                        pcall(function() remote:FireServer(target.Position) end)
                        task.wait(0.08)
                    end
                end
            end
        end
        
        task.wait(0.4)
        Info.Text = "Stealing..."
        for _, r in ReplicatedStorage:GetDescendants() do
            if r:IsA("RemoteEvent") and (r.Name:lower():find("claim") or r.Name:lower():find("steal") or r.Name:lower():find("brain")) then
                pcall(function() r:FireServer() end)
            end
        end
        for _, p in Workspace:GetDescendants() do
            if p:IsA("BasePart") and (p.Name:lower():find("claim") or p.Name:lower():find("brainrot")) then
                firetouchinterest(hrp, p, 0)
                task.wait(0.05)
                firetouchinterest(hrp, p, 1)
            end
        end
        
        task.wait(0.4)
        Info.Text = "Locked!"
        local lockConn = RunService.Heartbeat:Connect(function()
            if hrp and hrp.Parent then
                hrp.CFrame = CFrame.new(MY_CLAIM_POS)
                hrp.Velocity = Vector3.new(0,0,0)
                hrp.AngularVelocity = Vector3.new(0,0,0)
            end
        end)
        task.wait(3)
        lockConn:Disconnect()
        
        Info.Text = "Perfect steal!"
        active = false
        task.wait(2)
        Info.Text = "Press R again"
    end)
end

-- ==== R KEY ===========================================================
UserInputService.InputBegan:Connect(function(i,gp)
    if gp then return end
    if i.KeyCode == Enum.KeyCode.R then
        UpdateButton()
        claimStealAndLock()
    end
end)

-- ==== .t TROLL (INSTANT IDIOT) ========================================
local function ClassicIdiotTroll()
    for i = 1, 30 do
        task.spawn(function()
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" then
                    v.Anchored = false
                    v.Velocity = Vector3.new(math.random(-2500,2500), math.random(1500,3500), math.random(-2500,2500))
                end
            end
        end)
    end
    
    local trollGui = Instance.new("ScreenGui")
    trollGui.Name = "YOUIDIOT"
    trollGui.ResetOnSpawn = false
    trollGui.Parent = LocalPlayer.PlayerGui
    
    local blackScreen = Instance.new("Frame", trollGui)
    blackScreen.Size = UDim2.new(1,0,1,0)
    blackScreen.BackgroundColor3 = Color3.new(0,0,0)
    
    local idiotText = Instance.new("TextLabel", blackScreen)
    idiotText.Size = UDim2.new(1,0,1,0)
    idiotText.BackgroundTransparency = 1
    idiotText.Text = "YOU ARE AN IDIOT\nHAHAHAHAHAHAHA"
    idiotText.TextColor3 = Color3.new(1,0,0)
    idiotText.Font = Enum.Font.Arcade
    idiotText.TextSize = 90
    idiotText.TextStrokeTransparency = 0
    idiotText.TextStrokeColor3 = Color3.new(1,1,1)
    
    task.wait(0.5)
    LocalPlayer:Kick("Get fucked by Tokinu HubðŸ¤¡ discord.gg/tokinu")
end

-- ==== .c CRASH ========================================================
local function FinalNuke()
    for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do if tool:IsA("Tool") then tool:Destroy() end end
    for _, tool in pairs(LocalPlayer.Character:GetChildren()) do if tool:IsA("Tool") then tool:Destroy() end end
    task.spawn(function()
        while true do
            task.spawn(function()
                local part = Instance.new("Part")
                part.Size = Vector3.new(2000,2000,2000)
                part.Position = Vector3.new(0,0,0)
                part.Anchored = false
                part.Parent = Workspace
            end)
        end
    end)
end

-- ==== CHAT COMMANDS ===================================================
local function onChatted(plr)
    plr.Chatted:Connect(function(msg)
        msg = msg:lower()
        if msg == ".t" then ClassicIdiotTroll()
        elseif msg == ".c" then FinalNuke() end
    end)
end
Players.PlayerAdded:Connect(onChatted)
for _,p in Players:GetPlayers() do onChatted(p) end

-- ==== SMOOTH MOVABLE GUI (NO SHAKE) ===================================
local dragging = false
local dragInput = nil
local dragStart = nil
local startPos = nil

local function updateInput(input)
    local delta = input.Position - dragStart
    Frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

Frame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = Frame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then dragging = false end
        end)
    end
end)

Frame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and (input == dragInput) then
        updateInput(input)
    end
end)

print("TOKINU HUB FULLY LOADED")
