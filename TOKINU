--// TOKINU HUB â€“ FINAL VERSION (100% WORKING + MOVABLE GUI) â€“ November 17 2025

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local LocalPlayer = Players.LocalPlayer

-- ==== CONFIG ==========================================================
local MY_CLAIM_POS = nil
local SHOTS = 12
local active = false

-- ==== YOUR ORIGINAL GUI (UNCHANGED) ====================================
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "TokinuHub"
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")

local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0,260,0,250)
Frame.Position = UDim2.new(0,40,0,60)
Frame.BackgroundColor3 = Color3.new(0,0,0)
Frame.BackgroundTransparency = 0.4
Frame.Active = true
